{
  "name": "my_cat_list",
  "version": "1.0.0",
  "private": true,
  "author": "dimahvatit",
  "description": "SAPUI5 Online shop internship project",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "scripts": {
    "start": "npm-run-all --parallel express webapp",
    "test": "ui5 serve -o test/mockServer.html --accept-remote-connections",
    "webapp": "ui5 serve -o index.html --accept-remote-connections",
    "express": "node server/server.js",
    "proxy": "node proxy/proxy.js"
  },
  "devDependencies": {
    "@sap/ux-ui5-tooling": "^1.4.3",
    "@ui5/cli": "^2.14.4",
    "@ui5/fs": "^2.0.6",
    "@ui5/logger": "^2.0.1",
    "cors-anywhere": "^0.4.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "ui5": {
    "dependencies": [
      "@sap/ux-ui5-tooling"
    ],
    "ui5linter": {
      "severity": {
        "WrongParametersLinter": "Warning"
      }
    },
    "ui5parser": {
      "ui5version": "1.97.0",
      "dataSource": "https://sapui5.hana.ondemand.com/"
    }
  },
  "dependencies": {
    "cors": "^2.8.5",
    "http-proxy": "^1.18.1"
  }
}
