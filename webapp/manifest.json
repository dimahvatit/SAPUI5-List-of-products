{
	"_version": "1.37.0",

	"start_url": "index.html",

	"sap.app": {
		"id": "myCategoryList",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"categories": {
				"uri": "http://localhost:8081/https://services.odata.org/V2/Northwind/Northwind.svc/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "myCategoryList.view.App",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.93",
			"libs": {
				"sap.m": {
					"minVersion": "1.34.0"
				},
				"sap.ui.core": {
					"minVersion": "1.34.0"
				}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"uri": "i18n/i18n.properties",
				"settings": {
					"enhanceWith": [{
						"bundleUrl": "i18n/i18n.properties",
						"bundleUrlRelativeTo": "manifest"
					}],
					"bundleName": "myCategoryList.i18n.i18n",
					"supportedLocales": ["en", "ru_RU"],
					"fallbackLocale": "en",
					"async": "true"
				}
			},
			"category": {
				"dataSource": "categories"
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "myCategoryList.view",
				"controlId": "app",
				"controlAggregation": "pages"
			},
			"routes": [
				{
					"pattern": "",
					"name": "overview",
					"target": "overview"
				},
				{
					"pattern": "products/{productPath}",
					"name": "products",
					"target": "products"
				}
			],
			"targets": {
				"overview": {
					"viewId": "overview",
					"viewName": "Overview"
				},
				"products": {
					"viewId": "products",
					"viewName": "Products"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		}
	}
}
